@model IEnumerable<GreenZeta.com.Models.ScreenShot>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            id
        </th>
        <th>
            url
        </th>
        <th>
            caption
        </th>
        <th>
            projectId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ScreenShotID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.url)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.caption)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ScreenShotID }) |
            @Html.ActionLink("Details", "Details", new { id = item.ScreenShotID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ScreenShotID })
        </td>
    </tr>
}

</table>
